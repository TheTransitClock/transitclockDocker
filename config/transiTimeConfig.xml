<?xml version="1.0" encoding="UTF-8"?>
<transitime>
	<modules>
		<optionalModulesList>org.transitime.core.schedBasedPreds.SchedBasedPredsModule;org.transitime.avl.KombusAvlModule</optionalModulesList>
	</modules>

	<autoBlockAssigner>
		<autoAssignerEnabled>true</autoAssignerEnabled>
		<ignoreAvlAssignments>true</ignoreAvlAssignments>
		<allowableEarlySeconds>600</allowableEarlySeconds>
		<allowableLateSeconds>600</allowableLateSeconds>
	</autoBlockAssigner>

	<core>
		<predictionGeneratorClass>org.transitime.core.PredictionGeneratorDefaultImpl</predictionGeneratorClass>
		<beforeStopDistance>100</beforeStopDistance>
		<afterStopDistance>100</afterStopDistance>
		<holdingTimeGeneratorClass></holdingTimeGeneratorClass>
	</core>

	<predAccuracy>
		<maxPredTimeMinutes>30</maxPredTimeMinutes>
	</predAccuracy>
	<avl>
		<gtfsRealtimeFeedURI>GTFSRTVEHICLEPOSITIONS</gtfsRealtimeFeedURI>
		<url>GTFSRTVEHICLEPOSITIONS</url>
		<feedPollingRateSecs>10</feedPollingRateSecs>
		<minLongitude>14.76</minLongitude>
        <maxLongitude>24.50</maxLongitude>             
        <minLatitude>48.40</minLatitude>
        <maxLatitude>55.86</maxLatitude>
	</avl>
	<cache>
		<daysPopulateHistoricalCache>0</daysPopulateHistoricalCache>
	</cache>
	<db>
		<dbName>AGENCYNAME</dbName>
		<dbHost>POSTGRES_PORT_5432_TCP_ADDR:POSTGRES_PORT_5432_TCP_PORT
		</dbHost>
		<dbType>postgresql</dbType>
		<dbUserName>postgres</dbUserName>
		<dbPassword>PGPASSWORD</dbPassword>
	</db>
	<hibernate>
		<!-- <configFile>C:/cygwin64/home/SeanOg/workspace/transitime-historical/core/transitimeWebapp/src/main/resources/postgres_hibernate.cfg.xml</configFile> -->
		<configFile>/usr/local/transitime/config/hibernate.cfg.xml</configFile>
	</hibernate>
	<web>
		<mapTileUrl>http://tile.openstreetmap.org/{z}/{x}/{y}.png</mapTileUrl>
		<mapTileCopyright>OpenStreetMap contributors</mapTileCopyright>
	</web>
</transitime>
